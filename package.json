{
  "name": "ffc-pay-mi-reporting",
  "version": "1.1.3",
  "description": "Azure Function to create MI reports",
  "author": "Defra",
  "contributors": [
    "Steve Dickinson <steve.dickinson@defra.gov.uk>",
    "John Watson <john.watson2@rpa.gov.uk>",
    "Marc Templeton <m.templeton@kainos.com>",
    "Simon Dunn <simon.dunn1@defra.gov.uk>"
  ],
  "scripts": {
    "lint": "standard",
    "start": "func start -p 7085",
