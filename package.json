  "devDependencies": {
    "@types/jest": "28.1.6",
    "jest": "28.1.3",
    "jest-junit": "14.0.0",
    "standard": "17.0.0"
  },
  "homepage": "https://github.com/DEFRA/ffc-pay-mi-reporting",
  "repository": {
    "type": "git",
    "url": "https://github.com/DEFRA/ffc-pay-mi-reporting.git"
  },
  "standard": {
    "globals": [
      "describe",
      "beforeEach",
      "expect",
      "test",
      "afterEach",
      "jest",
      "beforeAll",
      "afterAll"
    ]
  }
}
